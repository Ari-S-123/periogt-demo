# Modal PerioGT API endpoint (from `modal serve` or `modal deploy`)
MODAL_PERIOGT_URL=https://your-workspace--periogt-api-periogt-api.modal.run

# Modal proxy auth credentials (workspace Proxy Auth token: wk/ws)
# Account API tokens (ak/as) will not work for proxy auth.
MODAL_KEY=wk_your_proxy_token_id
MODAL_SECRET=ws_your_proxy_token_secret
